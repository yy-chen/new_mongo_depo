systemLog:
    destination: file
    path: /home/dhcd/mongo/<%=@ParentName%>/<%=@Name%>/log/mongo.log
    logAppend: true
    timeStampFormat: iso8601-local

storage:
    dbPath: /home/dhcd/mongo/<%=@ParentName%>/<%=@Name%>/data
    engine: wiredTiger
    wiredTiger:
        engineConfig:
            cacheSizeGB: 1
    directoryPerDB: true
    journal:
        enabled: true

net:
    bindIp: <%=@Ip%>
    port: <%=@Port%>

replication:
    replSetName: <%=@ParentName%>

processManagement:
    fork: true
